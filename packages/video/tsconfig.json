{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "lib": ["ESNext"],
    "rootDirs": ["./src"],
    "incremental": false,
    "noEmit": true,
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "remotion-build"]
}
 